#:kivy 1.0.9

<ImagesScatter>:
    do_rotation: False
    do_translation: True
    do_scale: True
    auto_bring_to_front: False
    do_collide_after_children: True
    scale_min: 1.0
    scale_max: 10.0


<GameScreen>:
    size_hint: 1, 1
    size: root.width, root.height
    canvas:
        Color:
            rgba: 0.6, 0.3, 0.6, 1

        Rectangle:
            pos: self.pos
            size: self.size